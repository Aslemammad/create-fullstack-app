{
  "name": "website",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development node server",
    "build": "next build",
    "export": "next export",
    "start": "cross-env NODE_ENV=production node server",
    "format": "standardx \"./**/*.{js,jsx,ts,tsx}\" --fix | snazzy || true",
    "standard-check": "standardx \"./**/*.{js,jsx,ts,tsx}\" --verbose | snazzy || true"
  },
  "eslintConfig": {
    "rules": {
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "error"
    }
  },
  "standardx": {
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint/eslint-plugin"]
  },
  "dependencies": {
    "axios": "^0.19.2",
    "express": "^4.17.1",
    "express-http-to-https": "^1.1.4",
    "next": "9.4.4",
    "next-fonts": "^1.4.0",
    "next-pwa": "^3.1.0",
    "nextjs-redirect": "^5.0.0",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@types/node": "^14.0.25",
    "@types/react": "^16.9.43",
    "@types/styled-jsx": "^2.2.8",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "cross-env": "^7.0.2",
    "snazzy": "^8.0.0",
    "standardx": "^5.0.0",
    "typescript": "^3.9.7"
  }
}
